<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
	"http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap>

<resultMap id="MachineType" class="cn.ibm.com.entity.MachineType">
	 <!-- <result property="id" column="ID"/> -->
	 <result property="machine_type" column="MACHINE_TYPE"/>
	 <result property="create_time" column="CREATE_TIME"  />
	 <result property="create_user" column="CREATE_USER"  />
 </resultMap>
    <select id="listAllMachineType" resultMap="MachineType">  
SELECT
    MACHINE_TYPE,
    CREATE_TIME,
    CREATE_USER
FROM
    MACHINE_TYPE
order by machine_type asc
  </select>
 
 <delete id="deleteMachineType">
DELETE
FROM
    MACHINE_TYPE
WHERE
    MACHINE_TYPE = #machine_type#
 </delete>


 <insert id="insertMachineType" parameterClass="cn.ibm.com.entity.MachineType">
        INSERT INTO MACHINE_TYPE ( 
         <dynamic prepend="">
            <!-- <isNotEmpty prepend="" property="id">  ID  </isNotEmpty> -->
            <isNotEmpty prepend="" property="machine_type">  MACHINE_TYPE  </isNotEmpty>
            <isNotEmpty prepend="," property="create_time">  CREATE_TIME  </isNotEmpty>
            <isNotEmpty prepend="," property="create_user">  CREATE_USER  </isNotEmpty>
          </dynamic>         
         ) VALUES (
          <dynamic prepend="">
		    <!-- <isNotEmpty prepend="" property="id">  #id#  </isNotEmpty> -->
            <isNotEmpty prepend="" property="machine_type">  #machine_type#  </isNotEmpty>
            <isNotEmpty prepend="," property="create_time">  #create_time#  </isNotEmpty>
            <isNotEmpty prepend="," property="create_user">  #create_user#  </isNotEmpty>
           </dynamic>
         )
    </insert>
    
  <update id="updateMachineType" parameterClass="cn.ibm.com.entity.MachineType">  
      UPDATE MACHINE_TYPE       
        <dynamic prepend="SET">
            <!-- <isNotEmpty prepend="," property="id">  ID = #id#  </isNotEmpty> -->
            <!-- <isNotEmpty prepend="" property="user_id">  USER_ID = #user_id#  </isNotEmpty> -->
            <isNotEmpty prepend="," property="machine_type">  MACHINE_TYPE = #machine_type#  </isNotEmpty>
            <isNotEmpty prepend="," property="create_time">  CREATE_TIME = #create_time#  </isNotEmpty>
            <isNotEmpty prepend="," property="create_user">  CREATE_USER = #create_user#  </isNotEmpty>
        </dynamic>
       WHERE MACHINE_TYPE = #old_machine_type#
  </update>
    
</sqlMap>
